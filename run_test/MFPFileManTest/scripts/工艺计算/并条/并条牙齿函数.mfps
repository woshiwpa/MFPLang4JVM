function i_zhu311_320(i_zong,i_hou_yu_she)
variable i_zhu,i_hou,F,E,H,G,F_s=[71,77],G_s=[61,71],H_s=[58,75,81],s,ge_shu=0
for variable idx1=0 to 1 step 1
F=F_s[idx1]
E=100-F
   for variable idx2=0 to 1 step 1
   G=G_s[idx2]
      for variable idx3=0 to 2 step 1
      H=H_s[idx3]
      i_zhu=xiao_shu(36/25*F/E*H/G,3)
      i_hou=round(i_zong/1.018/i_zhu,3)
      if and(i_hou<=i_hou_yu_she+0.25,i_hou>=i_hou_yu_she-0.25)
      set_array_elem(s,ge_shu,[i_hou,i_zhu,F,E,G,H])
      
      ge_shu=ge_shu+1
      endif
      next
   next
next
print("━━━━━━━━━━━━━━━━━\n")
s=myf_sort_duo_wei(0,s)
for variable idx=0 to ge_shu-1 step 1
i_hou=round(s[idx,0],3)
i_zhu=round(s[idx,1],3)
F=s[idx,2]
E=s[idx,3]
G=s[idx,4]
H=s[idx,5]
print("i主="+i_zhu+"，i后="+i_hou+"，F/E="+F+"/"+E+"，G/H="+G+"/"+H+"\n\n")
next
print("━━━━━━━━━━━━━━━━━\n")
endf